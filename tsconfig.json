{
  "compilerOptions": {
    "target": "es5",
    "module": "commonjs",
    "noImplicitAny": false,
    "sourceMap": true,
    "declaration": true,
    "strictNullChecks": true,
    "outDir": "dist/es5",
    "declarationDir": "dist/typings",
    "allowSyntheticDefaultImports": true,
    "lib": [
      "es2015", "dom"
    ]
  },
  "exclude": [
    "node_modules"
  ]
}
