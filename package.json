{
  "name": "oddstream",
  "version": "1.0.5",
  "description": "A library to easily apply a unidirectional dataflow in your apps with RxJS.",
  "main": "index.js",
  "typings": "index.d.ts",
  "scripts": {
    "mkdist": "mkdirp dist",
    "build:js": "browserify -e index.js -o dist/oddstream.js",
    "build:umd": "browserify -e index.js -s oddstream | derequire > dist/oddstream.umd.js",
    "build": "npm-run-all mkdist --parallel build:js build:umd",
    "tsc": "tsc",
    "ava": "ava --tap | tap-nyan",
    "clean": "rimraf index.js index.d.ts index.js.map interface",
    "test": "npm-run-all tsc ava clean",
    "prepublish": "npm-run-all tsc build"
  },
  "author": "Kahlil Lechelt",
  "license": "MIT",
  "repository": "kahlil/oddstream",
  "dependencies": {
    "lodash.camelcase": "^4.1.1",
    "lodash.curry": "^4.0.1",
    "rxjs": "^5.0.0-beta.8"
  },
  "devDependencies": {
    "ava": "^0.15.1",
    "browserify": "^13.0.1",
    "derequire": "^2.0.3",
    "mkdirp": "^0.5.1",
    "npm-run-all": "^2.3.0",
    "rimraf": "^2.5.2",
    "tap-nyan": "0.0.2",
    "typescript": "^1.8.10",
    "uglify-js": "^2.6.2"
  }
}
